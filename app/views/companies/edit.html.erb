<%= simple_form_for @company, method: :patch do |f| %>
<%= f.input :name, label: 'Nom'%>
<%= f.input :address, label: 'Adresse'%>
<%= f.input :legal_form, label: 'Forme sociale' %>
<%= f.input :creation_date, as: :string, label: "Date d'immatriculation"%>
<%= f.input :number_of_shares, as: :string, label: "Nombre d'actions"%>
<%= f.input :share_nominal_value_cents, as: :string, label: "Valeur nominale de l'action"%>
<%= f.input :logo_url, as: :file, label: "Logo" %>
<%= f.input :logo_url_cache, as: :hidden %>
<%= f.submit 'Confirmer' %>
<% end %>
